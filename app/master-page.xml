<Page loaded="pageLoaded" class="page" xmlns="http://www.nativescript.org/tns.xsd">
	<ActionBar title="Master">
	</ActionBar>

	<ScrollView>
		<StackLayout class="home-panel">
            <Button text="tap tap" tap="{{ onTap }}" />
            <Button text="Navigate" tap="{{ onNavigate }}" class="btn"/>
            <Button text="Reload" tap="{{ reload }}" class="btn"/>
            <Label text="Label" textWrap="true"/>
		</StackLayout>
	</ScrollView>
</Page>
